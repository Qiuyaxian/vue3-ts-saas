{
  "name": "main",
  "version": "0.0.1",
  "scripts": {
    "serve": "npm run serve:dev",
    "serve:dev": "vite --open --mode dev",
    "serve:release": "vite --open --mode release",
    "serve:prod": "vite --open --mode prod",
    "build": "npm run build:dev",
    "build:dev": "node --max_old_space_size=1024000 ./node_modules/vite/bin/vite.js build --mode dev",
    "build:release": "node --max_old_space_size=1024000 ./node_modules/vite/bin/vite.js build --mode release",
    "build:prod": "node --max_old_space_size=1024000 ./node_modules/vite/bin/vite.js build --mode prod",
    "preview": "vite preview"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.6.8",
    "element-plus": "^2.7.2",
    "pinia": "^2.1.7",
    "pinia-plugin-persist": "^1.0.0",
    "vite-plugin-mock": "^3.0.2",
    "vue": "^3.2.47",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "mockjs": "^1.1.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.76.0",
    "sass-loader": "^13.3.3",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-progress": "^0.0.7",
    "vue-tsc": "^1.2.0"
  }
}
